<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fijicare.ViewPage.dashboard.Dashboard"
             NavigationPage.HasNavigationBar="False"
             xmlns:pages="clr-namespace:Fijicare.ViewPage.dashboard">
  <FlyoutPage.Flyout>
    <pages:DashboardFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:DashboardDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>